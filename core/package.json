{
  "name": "@zesty-io/core",
  "version": "0.0.23",
  "description": "This is a collection of React core UI components for the Zesty.io platform.",
  "author": "Zesty.io <developers@zesty.io>",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "scripts": {
    "test": "npm run build && jest",
    "precommit": "pretty-quick --staged",
    "prerelease": "npm run build && npm test",
    "release": "cp package.json dist/package.json && npm publish dist",
    "build:bundle": "npm run build:bundle:var && npm run build:bundle:umd",
    "build:bundle:var": "npx wp --config webpack.var-bundle.js",
    "build:bundle:umd": "npx wp --config webpack.umd-bundle.js",
    "build:components": "npx wp --config webpack.components.js",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --out-dir ./dist --ignore *.test.js",
    "prepare": "npx rimraf dist && npm run build:es2015"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "react": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-dom": "^16.4.1",
    "quill": "^1.3.6",
    "react-ace": "^6.1.2",
    "react-datepicker": "^1.5.0",
    "react-draft-wysiwyg": "^1.12.13",
    "react-quill": "^1.3.1",
    "draft-js": "^0.10.5",
    "moment": "^2.22.2",
    "@tinymce/tinymce-react": "^2.2.5"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.52",
    "@babel/core": "7.0.0-beta.52",
    "@babel/preset-env": "7.0.0-beta.52",
    "@babel/preset-react": "7.0.0-beta.52",
    "@babel/preset-stage-3": "7.0.0-beta.52",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "less": "^3.0.4",
    "prettier": "^1.13.6",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.6.2"
  }
}
